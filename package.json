{
  "name": "bananium",
  "version": "0.0.1",
  "description": "https://t.me/botoid",
  "private": true,
  "repository": {
    "url": "git+ssh://git@github.com/bananiumbot/bot",
    "type": "git"
  },
  "main": "index.js",
  "scripts": {
    "dev": "supervisor -x micro-bot index.js",
    "deploy": "now -e NODE_ENV=production -e BOT_TOKEN=$BOT_TOKEN --token $NOW_TOKEN --npm",
    "lint": "eslint .",
    "precommit": "npm test",
    "start": "micro-bot -d ${NOW_URL}",
    "test": "npm run lint"
  },
  "engines": {
    "node": ">=8.5.0"
  },
  "now": {
    "scale": {
      "bru1": {
        "min": 1,
        "max": 1
      }
    }
  },
  "author": "bananium authors",
  "license": "MIT",
  "dependencies": {
    "debug": "^3.0.0",
    "fuse.js": "^3.0.5",
    "micro-bot": "^2.2.2",
    "node-fetch": "^2.1.2",
    "telegraf-i18n": "^6.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "eslint": "^5.2.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "supervisor": "^0.12.0"
  }
}
